{
  "name": "@ts-defold/type-gen",
  "version": "0.1.1",
  "description": "Typescript type generator for Defold",
  "repository": "github:ts-defold/type-gen",
  "bugs": {
    "url": "https://github.com/ts-defold/type-gen/issues"
  },
  "keywords": [
    "typescript",
    "lua",
    "defold"
  ],
  "author": "Justin Walsh (@thejustinwalsh)",
  "homepage": "https://github.com/ts-defold/type-gen#readme",
  "license": "MIT",
  "main": "dist/build.js",
  "bin": "bin/build.js",
  "files": [
    "dist/**/*.js",
    "bin/**/*.js"
  ],
  "scripts": {
    "build": "tsc && npm run build-types",
    "build-types": "node dist/build.js",
    "stage": "tsc -m commonjs -lib es2019 -target es2019 -esModuleInterop -sourceMap --moduleResolution node build/index.ts && npm run stage-bin",
    "stage-bin": "node build/index.js"
  },
  "dependencies": {
    "got": "^11.7.0",
    "semver": "^7.3.2",
    "tmp-promise": "^3.0.2",
    "typescript": ">=4.0.2",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/got": "^9.6.11",
    "@types/node": "^13.7.7",
    "@types/tmp": "^0.2.0",
    "@types/unzip": "^0.1.1",
    "@types/unzipper": "^0.10.3",
    "lua-types": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0"
  }
}
